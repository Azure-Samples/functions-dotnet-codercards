{
  "bindings": [
    {
      "type": "queueTrigger",
      "queueName": "%input-queue%",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "name": "cardInfo"
    },
    {
      "type": "blob",
      "path": "%input-container%/{BlobName}",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "name": "image"
    },
    {
      "type": "blob",
      "connection": "AzureWebJobsStorage",
      "path": "%output-container%/{BlobName}",
      "direction": "out",
      "name": "outputBlob"
    }
  ],
  "disabled": false,
  "scriptFile": "../CoderCards.dll",
  "entryPoint": "CoderCardsLibrary.CardGenerator.GenerateCard"
}